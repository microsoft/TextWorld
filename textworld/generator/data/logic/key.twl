# key
type k : o {
    predicates {
        match(k, lockable);
    }

    constraints {
        k1 :: match(k, lockable) & match(k', lockable) -> fail();
        k2 :: match(k, lockable) & match(k, lockable') -> fail();
    }

    inform7 {
        type {
            kind :: "key";
        }

        predicates {
            match(k, lockable) :: "The matching key of the {lockable} is the {k}";
        }
    }
}
